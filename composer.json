    "description": "A Spamathon Counter for TarValon.net (HTML Scraper version)",
    "type": "project",
    "require": {
        "masterminds/html5": "^2.7",
        "league/csv": "^9.8",
        "guzzlehttp/guzzle": "^7.5",
